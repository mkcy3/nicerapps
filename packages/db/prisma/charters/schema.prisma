generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model Charter {
  stripePi String @id @unique
  clerkId  String
clerkEmail String
sleepAboard Boolean
passengers Int
  startDate DateTime @db.Date
  endDate   DateTime @db.Date

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
